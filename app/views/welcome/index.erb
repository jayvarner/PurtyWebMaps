
<div id="messages"></div>

<% flash.each do |key, msg| %>
    <%= content_tag :div, msg, :class => [key, " message"], :id => "notice_#{key}" %>
<% end %>
<% if flash[:notice] %>
  <p id="notice"><%= flash[:notice] %></p>
<% end %>
<%= form_tag '/welcome/create', { :multipart => true, :id => 'target' } do %>
    <p>
        <%= hidden_field_tag 'session[uuid]'%>
    <%= file_field_tag 'upload[file]' %>
    </p>
    <p>
    	<%= submit_tag "Upload" %>
    </p>
<% end %>
